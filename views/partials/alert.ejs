<div class="container">
    <% if(error && error.length > 0){ %>
    <div class="alert alert-danger mt-3" role="alert">
        <%= error %>
    </div>
    <% } %>
    <% if(success && success.length > 0){ %>
    <div class="alert alert-success mt-3" role="alert">
        <%= success %>
    </div>
    <% } %>
</div>